function refreshCats(innerHTML) {
  const cats = document.querySelector('.updates-cats');
  cats.innerHTML = innerHTML;
}

function moveTabs() {
  const catStatus = "<%= j @cat.status %>"
  document.getElementById(`nav-${catStatus}-tab`).click();
}


